{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Token": {
    "AccessTokenExpiresAfter": "10.00:00:00",
    "RefreshTokenExpiresAfter": "30.00:00:00"
  },
  "Database": {
    "CommandTimeOutInSeconds": 180,
    "DefaultSchema": "dbo",
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false
  },
  "ConnectionStrings": {
    "Cable": "Server=.;Initial Catalog=Cable;User ID=sa;Password=sa;Encrypt=True;TrustServerCertificate=True;"
  },
  "File": {
    "LimitSize": 5242880,
    "AllowedExtensions": [
      ".jpg",
      ".jpeg",
      ".png"
    ], 
    "ServerUrl": "http://localhost",
    "FileUploadPath":"C:\\inetpub\\wwwroot"
  }
}
